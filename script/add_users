#!/usr/bin/env ruby
# encoding: utf-8

require File.expand_path("../../config/environment",  __FILE__)

users = [
  "Tchami",
  "Kygo",
  "Jack U",
  "Kaskade",
  "Oliver Heldens",
  "Hannah Wants",
  "Gorgon City",
  "Adventure Club",
  "Chvrches",
  "Alex Metric",
  "Boys Noize",
  "Gesaffelstein",
  "Disclosure",
  "Dillon Francis",
  "DJ Hanzel",
  "Diplo"
]

$redis.multi
$redis.del("users")
users.each do |user|
  $redis.lpush("users", user)
end
$redis.exec